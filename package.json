{
	"name": "terminal-quick-tabs",
	"displayName": "Terminal Quick Tabs",
	"description": "Quickly open terminal from the status bar.",
	"publisher": "shaunakW",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/shaunakW/terminal-quick-tabs"
	},
	"bugs": {
		"url": "https://github.com/shaunakW/terminal-quick-tabs/issues"
	},
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"typescript": "^4.1.2"
	}
}
